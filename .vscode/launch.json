{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/BookKeeping/bin/Debug/net10.0/BookKeeping.dll",
      "args": [],
      "cwd": "${workspaceFolder}/BookKeeping",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:7245;http://localhost:5051"
      },
      "serverReadyAction": {
        "pattern": "Now listening on:\\s+(https?://\\S+)",
        "uriFormat": "%s",
        "action": "openExternally"
      },
      "launchBrowser": {
        "enabled": false
      }
    }
  ]
}
